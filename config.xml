<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.binsmobile8" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:android= "http://schemas.android.com/apk/res/android">
    <name>Rastreio Mobile</name>
    <description>
       Teste Rastreio
    </description>
    <author email="bins.br@gmail.com" href="http://www.leandrotonon.com.br/projetos/bins">
        Bins
    </author>

    <preference name="orientation" value="default" /> 
	<preference name="fullscreen" value="false" />
